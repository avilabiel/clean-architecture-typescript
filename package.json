  "scripts": {
    "test": "jest --ci --verbose --forceExit --detectOpenHandles",
    "start-dev": "nodemon --watch \"src\" --exec \"ts-node src/index.ts\" -e ts",
    "db:status": "sequelize db:migrate:status",
    "db:drop": "sequelize db:migrate:undo:all",
    "db:migrate": "sequelize db:migrate",
    "db:reset": "yarn db:drop && yarn db:migrate && yarn db:seed",
    "db:seed": "sequelize db:seed:all"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.6.2",
    "youch": "^2.2.2"
  }
}
